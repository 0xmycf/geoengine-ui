{
  "name": "WAVE",
  "version": "0.1.0",
  "scripts": {
    "postinstall": "bower install && typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "server": "http-server --proxy http://pc12316.mathematik.uni-marburg.de:10080/",
    "start": "concurrently \"npm run tsc:w\" \"npm run server\" ",
    "bundle": "node bundle.js",
    "doc": "typedoc --target ES5 --mode modules --experimentalDecorators --module system --excludeExternals --moduleResolution node --preserveConstEnums --stripInternal --suppressExcessPropertyErrors --suppressImplicitAnyIndexErrors --ignoreCompilerErrors --out ./doc ./src"
  },
  "license": "MIT",
  "dependencies": {
    "angular2": "2.0.0-beta.15",
    "dragula": "^3.6.8",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.35.0",
    "ng2-dragula": "^1.1.6",
    "ng2-material": "^0.3.6",
    "openlayers": "^3.15.1",
    "proj4": "^2.3.14",
    "reflect-metadata": "0.1.2",
    "rxjs": "5.0.0-beta.2",
    "systemjs": "0.19.26",
    "zone.js": "0.6.10"
  },
  "devDependencies": {
    "bower": "^1.7.9",
    "concurrently": "^2.0.0",
    "http-server": "^0.9.0",
    "systemjs-builder": "^0.15.14",
    "tslint": "3.6.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.10",
    "typings": "^0.7.12"
  }
}
